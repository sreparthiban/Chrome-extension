{
  "name": "Accu Bookmark",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
    },
	
  "browser_action": {
	"default_title": "Accu_BM",
	"default_icon": "accbm.png"
  },	 
  
  "permissions": [
    "storage",
	"clipboardWrite",
	"clipboardRead",
	"tabs",
	"background",
	"http://*/*",
	"https://*/*"
  ]
  
}